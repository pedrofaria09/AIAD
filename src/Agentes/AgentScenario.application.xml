<applicationtype xmlns="http://jadex.sourceforge.net/jadex"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jadex.sourceforge.net/jadex
http://jadex.sourceforge.net/jadexapplication-2.2.xsd"
	name="AgentScenario" package="Agentes">
	<componenttypes>
		<componenttype name="Bolsa" filename="bin/Agentes/BolsaAgentBDI.class"
			master="true" />
		<componenttype name="Investidor" filename="bin/Agentes/InvestidorAgent.class"
			master="true" />
	</componenttypes>
	<configurations>
		<configuration name="Initial-Test">
			<components>
				<component type="Bolsa" name="Bolsa" />
				<component type="Investidor" name="Investidor" />
			</components>
		</configuration>
	</configurations>
</applicationtype>